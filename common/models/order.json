      "description": "the dollar amount in cent per item"
    },
    "unit": {
      "type": "number",
      "default": 1,
      "required": true,
      "description": "the dollar amount in cent"
    },
    "currency_type": {
      "type": "string",
      "required": true,
      "description": "which type of currency for the customer did pay"
