{
  "name": "Basemap",
  "base": "PersistedModel",
  "idInjection": true,
  "properties": {
    "id": {
      "type": "string",
      "required": true
    },
    "complete": {
      "type": "number",
      "required": true
    },
    "total_zoom_levels": {
      "type": [
        {
          "scale": "number",
          "width": "number",
          "height": "number",
          "x": "number",
          "y": "number",
          "size": "number",
          "level": "number",
          "tiles": [
            {
              "x": "number",
              "y": "number",
              "tile": {
                "x": "number",
                "y": "number"
              }
            }
          ]
        }
      ],
      "required": false
    },
    "folder_base_name": {
      "type": "string",
      "required": true
    },
    "secret_base_map_file": {
      "type": "string",
      "required": true
    },
    "rename_file": {
      "type": "string",
      "required": true
    },
    "price": {
      "default": 0,
      "type": "number",
      "required": true
    },
    "currency": {
      "default": "HKD",
      "type": "string",
      "required": true
    },
    "owner": {
      "type": "string"
    },
    "image_type": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "artist": {
      "type": "belongsTo",
      "model": "Artist",
      "foreignKey": "publisherId"
    }
  },
  "acls": [],
  "methods": []
}
